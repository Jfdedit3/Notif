-- Charger la bibliothèque depuis l'URL
local GUILibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/Jfdedit3/Notif/refs/heads/main/Main.lua"))()

-- Vérifier si la fonction CreateGUI existe dans la bibliothèque chargée
if GUILibrary and GUILibrary.CreateGUI then
    -- Créer le GUI en utilisant la bibliothèque
    local myGui = GUILibrary:CreateGUI("MyCoolGUI")
else
    warn("La fonction CreateGUI n'existe pas dans cette bibliothèque !")
end
